{
  "search.exclude": {
    "*.exe": true,
  },
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "files.associations": {
    "*.Makefile": "makefile",
  },
  "editor.inlayHints.enabled": "off",
  "codeoptim.enableKeybindings": true,
}
