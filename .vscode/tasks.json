
{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Make perf",
      "type": "shell",
      "command": "make",
      "args": [
        "${fileBasenameNoExtension}.perf"
      ],
      "options": {
        "cwd": "${fileDirname}"
      }
    },
    {
      "label": "Make perf0",
      "type": "shell",
      "command": "make",
      "args": [
        "${fileBasenameNoExtension}.perf0"
      ],
      "options": {
        "cwd": "${fileDirname}"
      }
    },
    {
      "label": "Pdflatex",
      "type": "shell",
      "command": "pdflatex",
      "args": [
        "-interaction=nonstopmode",
        "${fileBasenameNoExtension}"
      ],
      "options": {
        "cwd": "${fileDirname}"
      }
    },
  ]
}
